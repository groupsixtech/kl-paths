*     Dictionary Form File : gspcuseg
*
.kl pcus
.form
$-,pcus
  &home &reverse "Polygon Dimensions" &standard &new_line
  &new_line
  " Filename:      " "-%12k" &new_line
  " Height:        " "-%6.2f" &new_line
  " Start X:       " "-%6.2f" &new_line
  " Start Y:       " "-%6.2f" &new_line
  " Polygon Rotation:   " "-%6.2f" &new_line
  " Polygon Pitch:      " "-%6.2f" &new_line
  ^pcus_fkey
.endform
$-,pcus_fkey     *function key labels
    "  EXIT" &new_line
    "  RUN" &new_line