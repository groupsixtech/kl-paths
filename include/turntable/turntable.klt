%ifndef turntable_t
%define turntable_t

TYPE
  tPAD FROM class_name = STRUCTURE
		overlap : REAL
    pad_offset : REAL
    pass_offset : REAL
    radius : REAL
    start_z  : REAL
    start_deg : REAL
    width_rot : REAL
    ccw : SHORT
    dir : SHORT
    idod : SHORT
