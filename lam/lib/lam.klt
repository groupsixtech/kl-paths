%ifndef lam_types_t
%define lam_types_t

--motion types
TYPE
  t_LASER FROM lamlib = STRUCTURE
    power : INTEGER
  ENDSTRUCTURE

  t_POWDER FROM lamlib = STRUCTURE
    hopper : INTEGER
    powder : INTEGER
    rpm : REAL
    lpm : REAL
    flow_rate : REAL
    height : REAL
  ENDSTRUCTURE

  t_DEPTHREGR FROM lamlib = STRUCTURE
    slope : REAL
    intercept : REAL
    low_limit : REAL
    high_limit : REAL
  ENDSTRUCTURE

%endif --lam_types_t