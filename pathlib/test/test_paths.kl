ROUTINE t_pvert_OD : BOOLEAN
  VAR
%define PAD_RADIUS 100
%define PAD_WIDTH 60
%define PAD_LENGTH 150
%defeval PAD_SEPERATION (30+PAD_WIDTH)
%define NO_OF_PADS 3
%defeval RETRACT_HEIGHT 20

