%define SAMPLE_START 100
%define SAMPLE_END 120
    i, j : INTEGER
    act_path, exp_path : ARRAY[11] OF XYZWPR
  BEGIN
    -- ** drawing **
    --draw pads
    clear_poly(polygon)
    tstpoly__new
    tstpoly__pads(PAD_WIDTH, PAD_LENGTH, PAD_SEPERATION, NO_OF_PADS, VEC2D(0,0) )

    --get raster lines
    -- (start_angle, dir, wall_lines, line_width, line_pitch)
