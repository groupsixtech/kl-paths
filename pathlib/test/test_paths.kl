    UNTIL( tstpln__is_path_end )

    --raise linking path
    paths__offset_toolpath(pth, POS(0,0,RETRACT_HEIGHT,0,0,0,(ZEROPOS(1).config_data)), PTH_NULL, PTH_LINKING)
    --convert coordinate systems
