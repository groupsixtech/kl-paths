        CASE(PTH_LINKING):
          tstpln__get_toolpath_interpolate(LINKING_STEP, PTH_LINETO, TRUE, pth)
      ENDSELECT

    UNTIL( tstpln__is_path_end )

    --convert coordinate systems
    paths__cylindrical_to_cartesian_mm(pth, origin, radius, Y_AXES)

    j = 1
